                <ui:Button text="START" name="play-button" class="button button--start"/>
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
    <ui:VisualElement name="resume-panel" class="overlay overlay--hidden">
        <ui:VisualElement class="dialog">
            <ui:Label text="SAVE DATA FOUND" class="dialog__title"/>
            <ui:Label text="Wave: 1&#10;Time: 00:00&#10;Score: 0" name="resume-info" class="dialog__info"/>
